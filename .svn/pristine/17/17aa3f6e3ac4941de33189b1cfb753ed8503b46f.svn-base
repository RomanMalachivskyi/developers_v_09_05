$(function() {
	
	var gameName = $("#gameName");
	
	var listOfPlayers = function() {
		$.ajax({
			url:"get-game-data",
			contentType: "application/x-www-form-urlencoded",
			data: {"message" : "get_game_name"},
			success: function(name){
				gameName.html(name);
			},
			error: function(error) {
				alert(error.responseText);
			}
		});
	};
	
	listOfPlayers();
	
});