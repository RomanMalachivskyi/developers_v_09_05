package com.epam.lab.developers.game;

import com.epam.lab.developers.entity.User;

public class GameJson {

	private String name;
	private User creator;
	private String  dateOfCreation;
	private boolean isJoinAvailable;
	
	public GameJson() {
		// TODO Auto-generated constructor stub
	}

	public GameJson(String name, User creator, String dateOfCreation,
			boolean isJoinAvailable) {
		super();
		this.name = name;
		this.creator = creator;
		this.dateOfCreation = dateOfCreation;
		this.isJoinAvailable = isJoinAvailable;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public User getCreator() {
		return creator;
	}

	public void setCreator(User creator) {
		this.creator = creator;
	}

	public String getDateOfCreation() {
		return dateOfCreation;
	}

	public void setDateOfCreation(String dateOfCreation) {
		this.dateOfCreation = dateOfCreation;
	}

	public boolean isJoinAvailable() {
		return isJoinAvailable;
	}

	public void setJoinAvailable(boolean isJoinAvailable) {
		this.isJoinAvailable = isJoinAvailable;
	}
	
	
}
