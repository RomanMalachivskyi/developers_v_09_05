package com.epam.lab.developers.game.map.unit;

import java.util.List;
import java.util.ArrayList;

import com.epam.lab.developers.game.map.algorithm_way.Step;
import com.epam.lab.developers.game.map.algorithm_way.WaveAlgorithm;

public class UnitTask {
	
	private int targetX;
	private int targetY;	
	private String task;
	
	private List<Step> way = new ArrayList<>();;
	
	public List<Step> getWay() {
		return way;
	}


	public void setWay(List<Step> way) {
		this.way = way;
	}


	public UnitTask(String task) {
		super();
		this.task = task;
	}
	
	
	public int getTargetX() {
		return targetX;
	}
	public int getTargetY() {
		return targetY;
	}
	public String getTask() {
		return task;
	}
	public void setTask(String task) {
		
		
		this.task = task;
		
	}
	

}
