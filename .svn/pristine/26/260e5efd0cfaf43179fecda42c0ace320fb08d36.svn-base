@CHARSET "UTF-8";

#gameHeader {
	position: absolute;
	margin: 0 auto;
	line-height:39px;
    font-family: Tahoma, Arial;
    background: url(../../img/theme/body_top_bckg.jpg) no-repeat center top #171c20;
    color:#bed7e6;
    font-size:11px;
    height: 370px;
    width: 1000px;
    left: 0;
    right: 0;
    z-index: 100;
}

#listOfcountUserOnline{
	 color:#fff;
    text-align:center;
    position: absolute;
	display: none;
	left: 0;
	top:15px;
	line-height:normal; 
	 text-align:left;
}

#listOfcountUserOnline li{
	margin-top: 5px;
	float: left;
	
}

#divMenuToggler {
	width: 1000px;
	height: 30px;
	left: 0;
    right: 0;
	margin: -33px auto 0 auto;
}

#menuToggler {
	display: none;
	width: 1000px;
	height: 30px;
	margin: 0 auto;
	background: url(../../img/theme/menu_toggle.jpg) -1px;
}

#gameContent {
	width: 1000px;
	margin: 30px auto 0 auto;
}

#gameMain {
	background: url(../../img/theme/body_all_bckg_game.jpg) repeat-y top center;
}

#gameName {
	margin-bottom: 10px;
	text-align: center;
    font-size: 16pt;
    font-family: "Trebuchet MS", "Verdana";
}

#divPlayers {
	height: 100px;
	margin-bottom: 20px;
    text-align: center;
    font-family: "Trebuchet MS", "Verdana";
}

#divPlayerYou {
	width: 100px;
	height: inherit;
	float: left;
	font-size: 10pt;
}

#youPlayerImg {
	width: 80px;
	margin: 0 auto;
}

.divPlayerOpponents {
	width: 100px;
	height: inherit;
	float: right;
	font-size: 10pt;
}

.opponentPlayerImg {
	width: 80px;
}

#divCanvas {
	position: relative;
	width: 1000px;
	margin: 0 auto;
}

#divOnCanvas {
	position: absolute;
	width: 1000px;
	height: 750px;
	margin: -750px auto 0 auto;
}

#activeObjectMenu {border: 1px solid grey;
	display: none;
	position: absolute;
	width: 100px;
}

.activeObjectMenuItem {
	text-align: center;
	margin: 0 auto;
	padding: 3px;
	width: 96px;
	font-size:14px;
	float: bottom;
	color:#fff;
	background-color:#445d6c;
	cursor: pointer;
}

.activeObjectMenuItem:hover {
	background-color:#f26513;
}

#unitOptions {
    width: 1000px;
    height: 100px;
	margin: 10px auto 0 auto;
}

#divUnitImg {
	width: 100px;
	float: left;
}

#unitImg {
	width: 100px;
}

#unitInfo {
	padding: 5px;
	height: 90px;
	float: left;
	font-family: "Trebuchet MS";
}

#unitName {
	font-size: 14pt;
}

#map_canvas{ 			/* встановлюємо ширину та висоту елементу canvas а також його межі*/
	
/*	width: 1000px;
    height: 750px;*/
}

#unit_canvas{
    margin-top: -750px auto 0 auto;
 /*   width: 1000px;
  height: 750px; */  
    border: 4px white solid;
}

#div_chat{
	background:#0D0C0C;
	position:absolute;
    top: 30%; /* Положение от нижнего края */
    right: 5%; /* Положение от правого края */
	position:fixed;
	font-size: 11pt;
	text-align: center;
	border: 3px solid #f2f2f2;
	width: 270px;
	height: 345px;
	padding: 10px;
	border-radius: 15px;
	margin: 10px;
}

#div_chat:hover{
	
	cursor: move;	
}


#output_chat{
	
	text-align: left;
	background-color: white;
	width: 257px;
	height: 200px;
	font-size: 11pt;
	line-height: 1.2;
	padding: 5px;
}

#input_chat{
	resize: none
}

#s_Send{
	
	width:268px;
	height: 35px;
	color: #fef4e9;
	border: solid 1px #003366;
	background: #3399ff;
	background: -webkit-gradient(linear, left top, left bottom, from(#3399ff), to(#003366));
	background: -moz-linear-gradient(top,  #3399ff,  #003366);
	filter:  progid:DXImageTransform.Microsoft.gradient(startColorstr='#3399ff', endColorstr='#003366');
}

#s_Send:hover{
	background: #003366;
	background: -webkit-gradient(linear, left top, left bottom, from(#f88e11), to(#f06015));
	background: -moz-linear-gradient(top,  #003366,  #3399ff);
	filter:  progid:DXImageTransform.Microsoft.gradient(startColorstr='#f88e11', endColorstr='#f06015');
	}
	
#s_Send:active{
	color: #fef4e9;
	border: solid 1px #003366;
	background: #3399ff;
	background: -webkit-gradient(linear, left top, left bottom, from(#3399ff), to(#003366));
	background: -moz-linear-gradient(top,  #3399ff,  #003366);
	filter:  progid:DXImageTransform.Microsoft.gradient(startColorstr='#3399ff', endColorstr='#003366');
	}
